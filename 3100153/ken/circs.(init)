(COMMENT list 50000. fixnum 40000. flonum 40000. symbol 20000.)
(PRINC '/;LOADING)
(SETQ ^W T)
(DEFPROP START-TECO (LISPT FASL DSK /.TECO/.) AUTOLOAD)
(defprop top (step fasl dsk ken) autoload)
(LAP TTY-ON SUBR)
(ARGS TTY-ON (NIL . 0))
(SETZ A)
(*OPEN 13 FOOBAR)
(JRST 0 TTY-ON1)
(MOVEI A 'T)
TTY-ON1 (POPJ P)
FOOBAR (SIXBIT / / /%T21)
NIL

(LAP CLICK SUBR)
(ARGS CLICK (NIL . 0))
(SETO TT)
(SETZ A)
(*IOT 13 TT)
(POPJ P)
NIL

(SETQ BASE 10. IBASE 10.)
(fasload colortv fasl dsk llogo)
(DEFPROP SD STARTDISPLAY EXPR)
(sd)
(ht)
(fasload dl fasl dsk ken)
(fancify)
(fasload inter fasl dsk ken)
(fasload circs)
(fasload cs)
(alloc '(flonum 40000.))
