(defun record-timings nil
 (let ((f (open '|time ings| 'append)))
      (dicks-print
       (mapcar
	(function (lambda (timing)
			  '(,(first timing) ,(second timing)
					    ,(times (third timing) .000001))))
	(get-all-times))
       f)
      (close f)))