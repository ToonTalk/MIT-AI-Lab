(defun do-invisibly (a)
 ((lambda (eraserstate)
	  (pu)
	  (mapc 'eval a)
	  (cond (eraserstate (erd))
		(t (pd))))
  :eraserstate ))

(DEFPROP HEADLINE
	 (LAMBDA (:SIZE) (PROG NIL
			      (do-invisibly
			       (list 
				'(rt 90.)
				(list 'bk (times :size 78.))
				'(lt 90.)))
			  10.  (TOPHALFHEAD :SIZE )
			  20.  (TOPHALFHEAD :SIZE )))
	 EXPR) 

	     
