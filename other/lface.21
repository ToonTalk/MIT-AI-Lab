(DEFPROP NOSE
	 (LAMBDA (:SIZE) (PROG NIL 
			  10.  (NOSELINE :SIZE)
			  (shoot?)
			  20.  (NOSTRIL :SIZE)
			       ))
	 EXPR) 
 

