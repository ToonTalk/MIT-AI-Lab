(COMMENT DEFAULT ALLOCATIONS CORE 100.) 
(PRINC '/;LOADING)
(SETQ ^W T)
(DEFPROP START-TECO (LISPT FASL DSK /.TECO/.) AUTOLOAD)
(defprop cs (tvrtle fasl dsk llogo) autoload)
(defprop sd (tvrtle fasl dsk llogo) autoload)
(defprop top (step fasl dsk ken) autoload)
(SETQ NOUUO T)
(SETQ BASE 10. IBASE 10.)
;(fasload goodys fasl)
(*RSET T)
(SETQ DEFUN T) 
(DEFPROP *DEFUN DEF-FORM GRINDFN)
(DEFPROP MACRODEF DEF-FORM GRINDFN)
(DEFPROP LEXPRDEF DEF-FORM GRINDFN)
(setq linel 94.) ;for the time being
(SETQ ^W NIL)