(DEFINE-FORM
 (RUN-FRAME CURRENT-APPEARANCES SPEED LAST-RUNTIME ACTION)
 (MAPC
  (FUNCTION
   (LAMBDA (CURRENT-APPEARANCE)
     (AND
      CURRENT-APPEARANCE
      (LET ((*MESSAGE '(!,ACTION))) (CASES (ACTOR-OF CURRENT-APPEARANCE))))))
  CURRENT-APPEARANCES)
 (SLEEP (-$ SPEED (//$ (FLOAT (- (RUNTIME) LAST-RUNTIME)) 1000000.0)))) 
(DEFINE-FORM
 (SHOW-NORMAL-MOVIE FIRST-FRAME LAST-FRAME SKIP-EVERY)
 (WIPE)
 (LET
  (
   (INCREMENT
    (COND ((< FIRST-FRAME LAST-FRAME) SKIP-EVERY)
          (T (- SKIP-EVERY)))))
  (DO
   ((FRAME-NUMBER FIRST-FRAME (+ FRAME-NUMBER INCREMENT))
    (LAST-RUNTIME (RUNTIME) (RUNTIME))
    (BEGIN-TIME (RUNTIME))
    (SPEED
     (//$
      (FLOAT
       (LET
        ((*MESSAGE '(RECALL YOUR SPEED)))
        (QUICK-COMPILED-ASK
         '#,(METHOD-AT-POSITION (|(RECALL YOUR ?)| 0) SOMETHING)
         '#,(ACTOR-OF (QUOTE MOVIE)))))))
    (NEW-FRAME-ACTION
     (LET
      ((*MESSAGE '(RECALL YOUR NEW-FRAME-ACTION)))
      (QUICK-COMPILED-ASK
       '#,(METHOD-AT-POSITION (|(RECALL YOUR ?)| 0) SOMETHING)
       '#,(ACTOR-OF (QUOTE MOVIE)))))
    (LAST-FRAME
     (MIN
      LAST-FRAME
      (LET
       ((*MESSAGE '(RECALL YOUR FRAME-COUNT)))
       (QUICK-COMPILED-ASK
        '#,(METHOD-AT-POSITION (|(RECALL YOUR ?)| 0) SOMETHING)
        '#,(ACTOR-OF (QUOTE MOVIE))))))
    (FRAME NIL)
    (PREVIOUS-FRAME NIL FRAME))
   (
    (COND ((> INCREMENT 0) (> FRAME-NUMBER LAST-FRAME))
          (T (< FRAME-NUMBER LAST-FRAME)))
    '(|I did it in| ,(//$ (FLOAT (- (RUNTIME) BEGIN-TIME)) 1000000.0) |cpu seconds.|))
   (SETQ FRAME
         (LET
          ((*MESSAGE '(RECALL YOUR (FRAME ,FRAME-NUMBER))))
          (QUICK-COMPILED-ASK
           '#,(METHOD-AT-POSITION (|(RECALL YOUR ?)| 0) SOMETHING)
           '#,(ACTOR-OF (QUOTE MOVIE)))))
   (PUT-IN-WHOLINE FRAME-NUMBER)
   (COND ((NULL FRAME))
         ((EQ NEW-FRAME-ACTION 'ERASE-OLD)
          (ERASE+DRAW-FRAME PREVIOUS-FRAME FRAME SPEED LAST-RUNTIME))
         ((EQ (FIRST NEW-FRAME-ACTION) 'ERASE-PREVIOUS)
          (ERASE+DRAW-FRAME
           (LET
            ((*MESSAGE
              '(RECALL YOUR (FRAME ,(- FRAME-NUMBER (SECOND NEW-FRAME-ACTION))))))
            (QUICK-COMPILED-ASK
             '#,(METHOD-AT-POSITION (|(RECALL YOUR ?)| 0) SOMETHING)
             '#,(ACTOR-OF (QUOTE MOVIE))))
           FRAME
           SPEED
           LAST-RUNTIME))
         (T (EVAL NEW-FRAME-ACTION) (RUN-FRAME FRAME SPEED LAST-RUNTIME '(DISPLAY))))))) 
