(DEFINE-FORM
 (DISPLAY-MOVIE-ALL-AT-ONCE MOVIE DOWN? COLUMNS X-SPACE Y-SPACE X-HOME Y-HOME SCALE)
 (CLEARSCREEN)
 (DO
  ((FRAME 0 (1+ FRAME))
   (LAST-FRAME
    (LET
     ((MYSELF (ACTOR-OF MOVIE)) (*MESSAGE '(RECALL YOUR FRAME-COUNT)))
     (QUICK-COMPILED-ASK
      '#,(METHOD-AT-POSITION (|(RECALL YOUR ?)| 0) SOMETHING)
      '#,(ACTOR-OF (QUOTE MOVIE))))))
  ((> FRAME LAST-FRAME))
  (PENUP)
  (LET
   ((*DIRECTOR-HOME-STATE
     (COND (DOWN?
            (LIST
             (FLOAT (PLUS (TIMES (// FRAME COLUMNS) X-SPACE) X-HOME))
             (FLOAT (PLUS (TIMES (\ FRAME COLUMNS) Y-SPACE -1.0) Y-HOME))))
           (T
            (LIST
             (FLOAT (PLUS (TIMES (\ FRAME COLUMNS) X-SPACE) X-HOME))
             (FLOAT (PLUS (TIMES (// FRAME COLUMNS) Y-SPACE -1.0) Y-HOME))))))
    (*DIRECTOR-TURTLE-SCALING-FACTOR (TIMES *DIRECTOR-TURTLE-SCALING-FACTOR SCALE)))
   (LET
    ((MYSELF (ACTOR-OF MOVIE)) (*MESSAGE '(PROJECT FRAME ,FRAME)))
    (QUICK-COMPILED-ASK
     '#,(METHOD-AT-POSITION (|(PROJECT FRAME ?)| 0) MOVIE)
     '#,(ACTOR-OF (QUOTE MOVIE))))))) 
