 (SHOW-MOVIE FIRST-FRAME LAST-FRAME SKIP-EVERY)
 (COND ((LET
         ((*MESSAGE '(RECALL YOUR COMPILED?)))
         (QUICK-COMPILED-ASK
          '#,(METHOD-AT-POSITION (|(RECALL YOUR ?)| 0) SOMETHING)
          '#,(ACTOR-OF (QUOTE MOVIE))))
