(DEFUN D0512 NIL
  ((LAMBDA (FILE-NAME)
     (LET
      ((CLONES-NAME (SAVE-COPY-OF-THE-MOVIE MYSELF FILE-NAME))
       (MOVIE-NAME (NAME-OF MYSELF)))
      (LET-FILES
       ((FILE-OBJECT (OPEN FILE-NAME 'APPEND)))
       (DO
        ((COMPILED-APPEARANCES NIL)
         (FRAMES NIL)
         (FRAME-COUNT
          (LET
           ((*MESSAGE '(RECALL YOUR FRAME-COUNT)))
           (QUICK-COMPILED-ASK
            '#,(METHOD-AT-POSITION (|(RECALL YOUR ?)| 0) SOMETHING)
            '#,(ACTOR-OF (QUOTE MOVIE)))))
         (FRAME-NUMBER 0 (1+ FRAME-NUMBER))
         (ARRAY-NAME (APPEND-ATOMS MOVIE-NAME '-FRAMES))
         (DRAW-PROCEDURES NIL))
        ((> FRAME-NUMBER FRAME-COUNT)
         (MAPC
          (FUNCTION
           (LAMBDA (DRAW-PROCEDURE)
             (DEFUNIZE
              DRAW-PROCEDURE
              (GET-INTERPRETIVE-DEFINITION DRAW-PROCEDURE)
              FILE-OBJECT)))
          DRAW-PROCEDURES)
         (PRINT-THE-MOVIE-ARRAY
          FRAMES
          MOVIE-NAME
          ARRAY-NAME
          CLONES-NAME
          FRAME-NUMBER
          FILE-OBJECT)
         NO-VALUE)
        (DO
         ((APPEARANCES
           (LET
            ((*MESSAGE '(RECALL YOUR (FRAME ,FRAME-NUMBER))))
            (QUICK-COMPILED-ASK
             '#,(METHOD-AT-POSITION (|(RECALL YOUR ?)| 0) SOMETHING)
             '#,(ACTOR-OF (QUOTE MOVIE))))
           (REST APPEARANCES))
          (FRAME))
         ((NULL APPEARANCES) (PUSH FRAME FRAMES))
         (LET
          ((COMPILED-APPEARANCE
            (LET
             ((MYSELF (ACTOR-OF (FIRST APPEARANCES))) (*MESSAGE '(COMPILE AS FUNCTION)))
             (QUICK-COMPILED-ASK
              '#,(METHOD-AT-POSITION ((COMPILE AS FUNCTION) 0) APPEARANCE)
              '#,(ACTOR-OF (QUOTE APPEARANCE))))))
          (PUSH
           (CONS
            (COND ((SECOND (ASSOC COMPILED-APPEARANCE COMPILED-APPEARANCES)))
                  (T
                   (LET
                    ((APPEARANCE-NAME (GENERATE-SYMBOL MOVIE-NAME)))
                    (SETQ DRAW-PROCEDURES
                          (UNION
                           DRAW-PROCEDURES
                           (LET
                            ((MYSELF (ACTOR-OF (FIRST APPEARANCES)))
                             (*MESSAGE '(RECALL YOUR DRAW-PROCEDURES)))
                            (QUICK-COMPILED-ASK
                             '#,(METHOD-AT-POSITION (|(RECALL YOUR ?)| 0) SOMETHING)
                             '#,(ACTOR-OF (QUOTE MOVIE))))))
                    (DEFUNIZE APPEARANCE-NAME COMPILED-APPEARANCE FILE-OBJECT)
                    (PUSH
                     (LIST COMPILED-APPEARANCE APPEARANCE-NAME)
                     COMPILED-APPEARANCES)
                    APPEARANCE-NAME)))
            (LET
             ((MYSELF (ACTOR-OF (FIRST APPEARANCES))) (*MESSAGE '(RECALL YOUR STATE)))
             (QUICK-COMPILED-ASK
              '#,(METHOD-AT-POSITION (|(RECALL YOUR ?)| 0) SOMETHING)
              '#,(ACTOR-OF (QUOTE MOVIE)))))
           FRAME)))))))
   (CADR *MESSAGE))) 
