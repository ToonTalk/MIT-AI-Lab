          (QUICK-COMPILED-ASK
           '#,(METHOD-AT-POSITION (|(RECALL YOUR ?)| 0) SOMETHING)
           '#,(ACTOR-OF (QUOTE MOVIE))))
         FIRST-FRAME
         (MIN
          LAST-FRAME
          (LET
           ((*MESSAGE '(RECALL YOUR FRAME-COUNT)))
           (QUICK-COMPILED-ASK
            '#,(METHOD-AT-POSITION (|(RECALL YOUR ?)| 0) SOMETHING)
            '#,(ACTOR-OF (QUOTE MOVIE)))))
         SKIP-EVERY
         (//$
          (FLOAT
           (LET
            ((*MESSAGE '(RECALL YOUR SPEED)))
            (QUICK-COMPILED-ASK
             '#,(METHOD-AT-POSITION (|(RECALL YOUR ?)| 0) SOMETHING)
             '#,(ACTOR-OF (QUOTE MOVIE))))))
         (LET
          ((*MESSAGE '(RECALL YOUR NEW-FRAME-ACTION)))
          (QUICK-COMPILED-ASK
           '#,(METHOD-AT-POSITION (|(RECALL YOUR ?)| 0) SOMETHING)
           '#,(ACTOR-OF (QUOTE MOVIE))))))
       (T (SHOW-NORMAL-MOVIE FIRST-FRAME LAST-FRAME SKIP-EVERY)))) 
